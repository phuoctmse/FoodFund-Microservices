{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/graphql-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/graphql-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/graphql-gateway",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "campaign": {
      "type": "application",
      "root": "apps/campaign",
      "entryFile": "main",
      "sourceRoot": "apps/campaign/src",
      "compilerOptions": {
        "tsConfigPath": "apps/campaign/tsconfig.app.json"
      }
    },
    "graphql-gateway": {
      "type": "application",
      "root": "apps/graphql-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/graphql-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/graphql-gateway/tsconfig.app.json"
      }
    },
    "operation": {
      "type": "application",
      "root": "apps/operation",
      "entryFile": "main",
      "sourceRoot": "apps/operation/src",
      "compilerOptions": {
        "tsConfigPath": "apps/operation/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}