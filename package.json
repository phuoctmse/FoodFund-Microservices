{
  "name": "FoodFund-Microservices",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "nest build",
    "build:auth": "nest build auth",
    "build:user": "nest build user",
    "build:gateway": "nest build graphql-gateway",
    "build:campaign": "nest build campaign",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "format:check": "prettier --check \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:auth": "nest start auth --watch",
    "start:user": "nest start user --watch",
    "start:gateway": "nest start graphql-gateway --watch",
    "start:campaign": "nest start campaign --watch",
    "start:prod": "node dist/apps/graphql-gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:cov": "jest --coverage --config jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.config.js",
    "prisma:generate": "prisma generate",
    "prisma:generate:user": "cd apps/user && npx prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:studio:user": "cd apps/user && npx prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:user": "cd apps/user && npx prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:deploy:user": "cd apps/user && npx prisma migrate deploy"
  },
  "dependencies": {
    "@apollo/gateway": "^2.11.2",
    "@apollo/server": "^4.9.5",
    "@apollo/subgraph": "^2.11.2",
    "@aws-sdk/client-cognito-identity-provider": "^3.454.0",
    "@aws-sdk/credential-providers": "^3.454.0",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "^6.14.0",
    "@sentry/nestjs": "^10.12.0",
    "@sentry/node": "^10.12.0",
    "aws-jwt-verify": "^4.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "graphql": "^16.8.1",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.14.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
